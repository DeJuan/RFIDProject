<Window x:Class="ThingMagic.URA2.Main"        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:ThingMagic.URA2" 
        xmlns:ucTagResults="clr-namespace:ThingMagic.URA2"
        xmlns:ucWriteEPC="clr-namespace:ThingMagic.URA2"
        xmlns:ucTagInspector="clr-namespace:ThingMagic.URA2"
        xmlns:ucUserMemory="clr-namespace:ThingMagic.URA2"
        xmlns:ucLockTag="clr-namespace:ThingMagic.URA2"
        Title="Universal Reader Assistant" Icon="/UniversalReaderAssistant;component/Icons/thing-magic-icon.ico" WindowStartupLocation="CenterScreen" Closing="Window_Closing" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"  d:DesignHeight="768" d:DesignWidth="1024" DataContext="{Binding ElementName=tcTagResults}">
    <DockPanel>
        <Grid DockPanel.Dock="Top" Name="stackPanel13" VerticalAlignment="Top" Height="48">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="0" />
                    <GradientStop Color="#C4D9EC" Offset="1" />
                </LinearGradientBrush>
            </Grid.Background>
            <Image Grid.Column="0" Name="image1" Source="..\Icons\ThingMagic_logo.png" MouseDown="image1_MouseDown" />
            <StackPanel Grid.Column="1" Orientation="Horizontal" Margin="12,9,0,9">
                <Button ToolTipService.ShowOnDisabled="True" Foreground="Black"  Content="Connect..."  Margin="10,0,0,0" Height="29" Width="90"  ToolTip="Connect" Name="btnConnectExpander" Click="btnConnectExpander_Click" />
                <Label ToolTipService.ShowOnDisabled="True" HorizontalContentAlignment="Center"  Content="Reader URI" Height="28" Name="lblReaderUri" Width="85" Foreground="Black" HorizontalAlignment="Center">
                        <Label.ToolTip>
                            Reader URI
                        </Label.ToolTip>
                </Label>                
                <Image Name="imgReaderStatus" Width="25" Height="25" HorizontalAlignment="Center" VerticalAlignment="Center" Source="/UniversalReaderAssistant;component/Icons/LedRed.png"></Image>
                <Label Name="lblshowStatus" Content="Disconnected"  HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" FontFamily="Arial"  ToolTip="Reader Status" HorizontalContentAlignment="Left" VerticalContentAlignment="Center"></Label>               
                <Button ToolTipService.ShowOnDisabled="True" Visibility="Hidden"  Content="Read" Height="29" Name="btnRead" Click="btnRead_Click" Width="90" IsEnabled="False" HorizontalContentAlignment="Center">
                    <Button.ToolTip>
                        Perform read based on the read behavior defined by the settings in the "Read/Write Options" sidebar.
                    </Button.ToolTip>
                </Button>
                <ToolBar Height="30">
                    <ToolBar.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="White" Offset="0" />
                            <GradientStop Color="#C4D9EC" Offset="1" />
                        </LinearGradientBrush>
                    </ToolBar.Background>
                    <Button ToolTipService.ShowOnDisabled="True" IsEnabled="False" ToolTip="Clear Tag Reads" HorizontalAlignment="Left" Name="btnClearTagReads" Click="btnClearReads_Click" VerticalAlignment="Top" >
                        <Image Name="toolbarClearTagReadsImage" Source="..\Icons\Clear.png" />
                    </Button>
                    <Separator/>
                    <Button ToolTipService.ShowOnDisabled="True" IsEnabled="False" ToolTip="Save Tag Reads" HorizontalAlignment="Left" Name="saveData" VerticalAlignment="Top" Click="saveData_Click" >
                        <Image Name="toolbarSaveTagReadsImage" Source="..\Icons\Save.png" />
                    </Button>
                </ToolBar>
                <Image Height="25" Stretch="Fill" Width="25" Source="/UniversalReaderAssistant;component/Icons/broadcast-icon-off.png" Name="imgTcpStreamStatus" >
                    <Image.ToolTip>
                        <ToolTip Content="Tcp streaming status"/>
                    </Image.ToolTip>
                </Image>
            </StackPanel>
            
        </Grid>
        <StackPanel Height="4" Grid.Row="0" Orientation="Horizontal" DockPanel.Dock="Top">
            <StackPanel.Background>
                #C4D9EC
            </StackPanel.Background>            
        </StackPanel>       
        <StatusBar HorizontalContentAlignment="Left" Height="30" Name="statusBar1" DockPanel.Dock="Bottom">
            <Label Name="lblWarning" FontSize="10" HorizontalContentAlignment="Center">
            </Label>
        </StatusBar>
        <!-- The bar of buttons docked along the right edge: -->
        <StackPanel Name="buttonBar" Orientation="Horizontal" DockPanel.Dock="Right">
            <StackPanel.LayoutTransform>
                <RotateTransform Angle="90"/>
            </StackPanel.LayoutTransform>
            <Button Name="pane1Button" MouseEnter="pane1Button_MouseEnter" FontWeight="Normal" FontSize="13" Width="132" FontFamily="Arial">    
                <StackPanel Orientation="Horizontal">
                        <Image Source="..\Icons\Settings.png" Stretch="None" />
                        <TextBlock Margin="5,0,0,0">Settings/Status</TextBlock>
                </StackPanel>
            </Button>
        </StackPanel>
        <!-- The Grid containing the three child Grids fills the DockPanel: -->
        <Grid Name="parentGrid" Grid.IsSharedSizeScope="True">
            <!-- Layer 0: -->
            <Grid Name="layer0" MouseEnter="layer0_MouseEnter" Width="Auto" >                
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300*" />
                    <ColumnDefinition Width="150*" SharedSizeGroup="maincolu" />
                </Grid.ColumnDefinitions>
                <TabControl Name="tcTagResults" FontFamily="Arial" Background="#9CAAC1">
                    <TabItem Header="Tag Results" Name="tiTagResults" GotFocus="tiTagResults_GotFocus" >
                        <TabItem.ContextMenu>
                            <ContextMenu  Name="ctMenu" Visibility="Collapsed">
                                <MenuItem Header="Write EPC" Click="MenuItem_Click" />
                                <MenuItem Header="Inspect Tag" Click="MenuItem_Click" />
                                <MenuItem Header="Edit User Memory" Click="MenuItem_Click" />
                                <MenuItem Header="Lock Tag" Click="MenuItem_Click"/>
                            </ContextMenu>
                        </TabItem.ContextMenu>
                        <Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding ElementName=bigNumTotalTagCounts, Path=Visibility}" Name="gridTotalTagCountsBigNum"  Margin="0,55,0,0" Panel.ZIndex="1000" Background="Transparent" Opacity=".5">
                                <Label Visibility="{Binding ElementName=bigNumTotalTagCounts, Path=Visibility}" Panel.ZIndex="1000" Margin="0,108,0,0" FontSize="32" Background="Transparent" Name="lblTotalTagCounts" Content="Total Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Visibility="Collapsed" HorizontalContentAlignment="Center" Panel.ZIndex="1000" Name="bigNumTotalTagCounts" Background="Transparent" Foreground="DarkViolet" Opacity=".5" FontSize="220" Content="{Binding ElementName=txtTotalTagReads, Path=Content}" Height="333" Margin="0,111,0,0"></Label>
                            </Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding ElementName=bigNumUniqueTagCounts, Path=Visibility}" Name="gridUniqueTagCountsBigNum" Margin="0,55,0,0"  Panel.ZIndex="1000" Background="Transparent" Opacity=".5">
                                <Label Visibility="{Binding ElementName=bigNumUniqueTagCounts, Path=Visibility}" Panel.ZIndex="1000" Margin="0,105,0,0" FontSize="32" Background="Transparent" Name="lblUniqueTagCounts" Content="Unique Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Visibility="Collapsed" HorizontalContentAlignment="Center" Panel.ZIndex="1000" Name="bigNumUniqueTagCounts" Background="Transparent" Foreground="DarkViolet" Opacity=".5" FontSize="220" Content="{Binding ElementName=totalUniqueTagsReadTextBox, Path=Content}" Height="333" Margin="0,111,0,0"></Label>
                            </Grid>
                            <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed" Name="gridCountsBigNum"  Panel.ZIndex="1000" Background="Transparent" Opacity=".5" Margin="0,51,0,0">
                                <Label Panel.ZIndex="1000" Margin="40,0,0,20" FontSize="32" Background="Transparent" Name="gridlblUniqueTagCounts" Content="Unique Tag Count" Foreground="DarkViolet" HorizontalContentAlignment="Center" ></Label>
                                <Label Foreground="DarkViolet" Panel.ZIndex="1000" Margin="0,10" FontSize="140" Background="Transparent" Opacity=".8" Name="gridlblUniqueTagCountsValue" Content="{Binding ElementName=totalUniqueTagsReadTextBox, Path=Content}" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="40,150,-1,11" Panel.ZIndex="1000" FontSize="32" Background="Transparent" Name="gridlblTotalTagCounts" Content="Total Tag Count" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="0,156,-2,10" Panel.ZIndex="1000" FontSize="140" Background="Transparent" Opacity=".8" Name="gridlblTotalTagCountsValue" Content="{Binding ElementName=txtTotalTagReads, Path=Content}" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="40,284,-1,11" Panel.ZIndex="1000" FontSize="32" Background="Transparent" Name="gridlblReadRatePerSecCount" Content="Read Rate/Sec" HorizontalContentAlignment="Center"></Label>
                                <Label Foreground="DarkViolet" Margin="0,294,-2,10" Panel.ZIndex="1000" FontSize="140" Background="Transparent" Opacity=".8" Name="gridlblReadRatePerSecCountValue" Content="{Binding ElementName=txtbxReadRatePerSec, Path=Content}" HorizontalContentAlignment="Center"></Label>
                            </Grid>
                            <ucTagResults:ucTagResults x:Name="TagResults" Width="auto" DataContext="{Binding}" />
                            </Grid>                        
                    </TabItem> 
                    <TabItem Header="Write EPC" Name="tiWriteEPC" Background="Aqua" GotFocus="tiWriteEPC_GotFocus">
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucWriteEPC:ucWriteEPC x:Name="WriteEpc" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Tag Inspector" Name="tiTagInspector" Background="Aqua" GotFocus="tiTagInspector_GotFocus" >
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">                                
                                <ucTagInspector:ucTagInspector x:Name="TagInspector" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Header="User Memory" Name="tiUserMemory" Background="Aqua" GotFocus="tiUserMemory_GotFocus" >
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucUserMemory:UserMemory x:Name="UserMemory" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                    <TabItem Header="Lock Tag" Name="tiLockTag" Background="Aqua" GotFocus="tiLockTag_GotFocus" >
                        <Grid Background="#E2EEFA">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <ucLockTag:ucLockTag x:Name="LockTag" Width="auto" DataContext="{Binding}"  />
                            </ScrollViewer>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
            <!-- Layer 1: -->
            <Grid Name="layer1" Visibility="Collapsed" DataContext="{Binding}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition SharedSizeGroup="column1" Width="370"/>
                </Grid.ColumnDefinitions>
                <!-- Column 0 is empty, but column 1 contains a Grid and a GridSplitter: -->
                <Border BorderThickness="1" Grid.Column="1" CornerRadius="10">
                    <Grid Grid.Column="1" Background="#BBC4D8">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <!-- Row 0 contains a header, and row 1 contains pane-specific content: -->
                    <DockPanel Grid.Row="0">
                            <Button Background="#D9DDE7" BorderBrush="Transparent" Name="pane1Pin" Width="26" ToolTip="Auto Hide" DockPanel.Dock="Right" Click="pane1Pin_Click">
                            <Image Name="pane1PinImage" Source="..\Icons\pinHorizontal.gif"/>
                        </Button>
                        <TextBlock  Padding="8" FontWeight="Bold" TextTrimming="CharacterEllipsis" Foreground="{StaticResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" DockPanel.Dock="Left" FontFamily="Arial">
                            <TextBlock.Background>
                                <LinearGradientBrush>
                                        <GradientStop Color="#DCE0E8" Offset="0"/>
                                        <GradientStop Color="#D9DDE7" Offset="1"/>
                                </LinearGradientBrush>                                
                            </TextBlock.Background>                            
                            <StackPanel Orientation="Horizontal">
                                    <Image Source="..\Icons\Settings.png" Stretch="None" />
                                    <TextBlock Margin="5,0,0,0">Settings/Status</TextBlock>
                            </StackPanel>
                        </TextBlock>
                    </DockPanel>
                        <ScrollViewer Name="settingsScrollviewer"  HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Grid.Row="1">
                            <StackPanel Grid.Row="1">
                                <Expander Collapsed="expdrConnect_Expanded" Expanded="expdrConnect_Expanded" Header="Connect" Name="expdrConnect" BorderBrush="Black" BorderThickness="1" FontFamily="Arial" >
                                    <Expander.Background>
                                        <LinearGradientBrush EndPoint="0.8,1" StartPoint="0.5,0">
                                            <GradientStop Color="#ddd" Offset="0" />
                                            <GradientStop Color="white" Offset="1" />
                                        </LinearGradientBrush>
                                    </Expander.Background>
                                    <Grid Background="White" x:Name="gridConnect">
                                        <Border Margin="3,1,1,0" BorderBrush="Black" BorderThickness="1">
                                            <Grid>
                                                <ComboBox ToolTip="Enter reader IPaddress/Hostname" Name="cmbFixedReaderAddr" ToolTipService.ShowOnDisabled="True" Width="106" Height="25" VerticalAlignment="Top" Margin="147,9,0,0" HorizontalAlignment="Left" Text="" SelectedIndex="0" IsEditable="True" FontFamily="Arial" Visibility="Visible" />
                                                <ComboBox ToolTip="Enter reader com port" Name="cmbReaderAddr" ToolTipService.ShowOnDisabled="True" Width="106" Height="25" VerticalAlignment="Top" Margin="147,9,0,0" HorizontalAlignment="Left" Text="" SelectedIndex="0" IsEditable="True" FontFamily="Arial" Visibility="Hidden" />
                                                <!--<TextBox ToolTip="Enter reader IP address" Name="txtReaderAddr" ToolTipService.ShowOnDisabled="True" Width="106" Height="25" VerticalAlignment="Top" Margin="147,9,0,0" HorizontalAlignment="Left" Text="" FontFamily="Arial" />-->
                                                <Button ToolTipService.ShowOnDisabled="True" Content="Connect"  Margin="258,7,0,0" Height="25" Width="70"  ToolTip="Connect" Name="btnConnect" Click="btnConnect_Click" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                                <RadioButton Content="Network Reader" ToolTip="Select this for Fixed Readers" ToolTipService.ShowOnDisabled="True" HorizontalAlignment="Left" Margin="5,9,0,0"  Name="rdbtnNetworkConnection" VerticalAlignment="Top" Checked="rdbtnNetworkConnection_Checked" Height="26" Width="126" IsChecked="True" />
                                                <RadioButton Margin="5,37,0,0" Content="Serial Reader" Height="25" Name="rdbtnLocalConnection" IsChecked="false" ToolTip="Select this for Serial Readers" ToolTipService.ShowOnDisabled="True" Checked="rdbtnLocalConnection_Checked" VerticalAlignment="Top" HorizontalAlignment="Left" Width="126" />
                                                <Button ToolTipService.ShowOnDisabled="True" Visibility="Collapsed" Content="Web UI"  Margin="258,38,0,0" Height="25" Width="70"  ToolTip="Launch web UI" Name="btnWebUI"  HorizontalAlignment="Left" VerticalAlignment="Top" Click="btnWebUI_Click" />
                                                <Button ToolTipService.ShowOnDisabled="True" 
                                                        Visibility="Visible" ToolTip="Refresh host name list"  
                                                        Content="Refresh" Name="btnRefreshReadersList" 
                                                        Margin="147,37,0,0" Height="25" Width="70" 
                                                        Click="btnRefreshReadersList_Click" VerticalAlignment="Top" 
                                                        HorizontalAlignment="Left"></Button>
                                                <CheckBox Content="Transport Logging" HorizontalAlignment="Left" Margin="140,75,0,0" Name="chkEnableTransportLogging" ToolTip="Enable Transport Logging." ToolTipService.ShowOnDisabled="True" Height="26" VerticalAlignment="Top" />
                                                <Button Visibility="Collapsed" Content="Probe Reader Names" Height="27" HorizontalAlignment="Left" Margin="7,118,0,0" Name="probeReadNames" VerticalAlignment="Top" Width="137" />
                                                <ComboBox Height="25" Width="67" IsEnabled="False" HorizontalAlignment="Left" Margin="70,70,0,0" Name="regioncombo" SelectedIndex="0" VerticalAlignment="Top"  SelectionChanged="regioncombo_SelectionChanged_1" />
                                                <Label Content="Region" HorizontalAlignment="Left" Margin="5,70,0,0" Name="lblRegion" Width="50" Height="28" VerticalAlignment="Top" />
                                                <StackPanel Orientation="Vertical" Margin="2,100,2,10" VerticalAlignment="Top" >
                                                    <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Left">
                                                        <Label Visibility="Collapsed" Content="Baud Rate" FontFamily="Arial" 
                                                               HorizontalAlignment="Left" Name="lblbaudRate" OpacityMask="#FFDCAFAF" 
                                                               Width="68" Height="28" VerticalAlignment="Top" />
                                                        <ComboBox Visibility="Collapsed" BorderBrush="Black" BorderThickness="1" 
                                                                  FontFamily="Arial" Height="25" Width="67" HorizontalAlignment="Left" 
                                                                  Name="cbxBaudRate" SelectedIndex="0" SelectedValuePath="Content" 
                                                                  VerticalAlignment="Top" SelectionChanged="cbxBaudRate_SelectionChanged_1">
                                                            <ComboBoxItem Content="Select" />
                                                            <ComboBoxItem Content="9600" />
                                                            <ComboBoxItem Content="19200" />
                                                            <ComboBoxItem Content="38400" />
                                                            <ComboBoxItem Content="115200" />
                                                            <ComboBoxItem Content="230400" />
                                                            <ComboBoxItem Content="460800" />
                                                            <ComboBoxItem Content="921600" />
                                                        </ComboBox>
                                                    </StackPanel>
                                                    <GroupBox Name="grpbxLoadSaveConfig" BorderThickness="1" BorderBrush="Black"
                                                              VerticalAlignment="Top" Header="Load/Save Profile">
                                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                                                            <Button Margin="0,2,4,2" ToolTipService.ShowOnDisabled="True" 
                                                                    ToolTip="Load reader and UI configuration parameters from a .urac file" 
                                                                    Name="btnLoadConfig" Width="120" Height="25" IsEnabled="True" 
                                                                    HorizontalAlignment="Left" VerticalAlignment="Top" Content="Load" 
                                                                    Click="btnLoadConfig_Click" ></Button>
                                                            <Button Margin="0,2,2,2" ToolTipService.ShowOnDisabled="True" 
                                                                    ToolTip="Save reader and UI configuration parameters to a .urac file" 
                                                                    IsEnabled="False" Name="btnSaveConfig" Width="120" Height="25" 
                                                                    HorizontalAlignment="Left" VerticalAlignment="Top" Content="Save" 
                                                                    Click="btnSaveConfig_Click" ></Button>
                                                        </StackPanel>
                                                    </GroupBox>
                                                </StackPanel>
                                            </Grid>
                                        </Border>
                                    </Grid>
                                </Expander>
                                <Expander Expanded="expdrPerformanceTuning_Expanded" Header="Read/Write Options" Name="expdrReadOptions" BorderBrush="Black" BorderThickness="1" FontFamily="Arial" IsExpanded="False">
                                    <Expander.Background>
                                        <LinearGradientBrush EndPoint="0.8,1" StartPoint="0.5,0">
                                            <GradientStop Color="#ddd" Offset="0" />
                                            <GradientStop Color="white" Offset="1" />
                                        </LinearGradientBrush>
                                    </Expander.Background>
                                    <Grid Background="White" x:Name="gridReadOptions" IsEnabled="False">
                                        <Grid.Resources>
                                            <BooleanToVisibilityConverter x:Key="boolToVisible"/>
                                        </Grid.Resources>
                                        <Border Margin="3,1,1,1" BorderBrush="Black" BorderThickness="1">
                                            <StackPanel Margin="2" Orientation="Vertical" Name="stckPnlRdBehaviour">
                                                <GroupBox Header="Read Behaviour" Name="grpbxReadBehaviour" VerticalAlignment="Top" BorderThickness="1" BorderBrush="Black">
                                                    <Grid Name="gridReadBehaviour" >
                                                        <Label Visibility="Visible" Content="RF On (ms)" Height="28" HorizontalAlignment="Left" Margin="137,5,0,0" Name="lblRfOn" VerticalAlignment="Top" />
                                                        <Label Visibility="Visible" Content="RF Off (ms)" Height="28" HorizontalAlignment="Left" Margin="137,35,0,0" Name="lblRfOff" VerticalAlignment="Top" />
                                                        <TextBox MaxLength="5" Visibility="Visible" HorizontalAlignment="Left" Margin="215,4,0,0" Name="txtRFOnTimeout" Width="67" Text="1000" Height="23" VerticalAlignment="Top" PreviewTextInput="txtRFOnTimeout_PreviewTextInput" TextChanged="timeout_TextChanged" LostKeyboardFocus="txtRFOnTimeout_LostKeyboardFocus" />
                                                        <TextBox MaxLength="5" Visibility="Visible" Height="23" Margin="215,34,0,0" Name="txtRFOffTimeout" VerticalAlignment="Top" HorizontalAlignment="Left" Width="67" Text="0" TextChanged="readDelay_TextChanged" PreviewTextInput="txtRFOnTimeout_PreviewTextInput" LostKeyboardFocus="txtRFOffTimeout_LostKeyboardFocus" />
                                                        <RadioButton ToolTipService.ShowOnDisabled="True" GroupName="ReadBehaviour" Content="Read Once" Height="16" HorizontalAlignment="Left" Margin="1,60,0,0" Name="rdBtnReadOnce" VerticalAlignment="Top" Checked="rdBtnReadOnce_Checked">
                                                            <RadioButton.ToolTip>
                                                                Perform sync read with the specified [timeout] ms.
                                                            </RadioButton.ToolTip>
                                                        </RadioButton>
                                                        <RadioButton ToolTipService.ShowOnDisabled="True" GroupName="ReadBehaviour" Content="Read Continuously" Height="16" HorizontalAlignment="Left" Margin="1,20,0,0" Name="rdBtnReadContinuously" VerticalAlignment="Top" Checked="rdBtnReadContinuously_Checked" IsChecked="True">
                                                            <RadioButton.ToolTip>
                                                                Perform async read with the specified [RF On and Off time] ms.
                                                            </RadioButton.ToolTip>
                                                        </RadioButton>
                                                        <Label Visibility="Collapsed" Content="Timeout (ms)" Height="28" HorizontalAlignment="Left" Margin="129,55,0,0" Name="lblReadOnce" VerticalAlignment="Top" />
                                                        <TextBox MaxLength="6" Visibility="Collapsed" HorizontalAlignment="Left" Margin="216,55,0,0" Name="txtbxreadOnceTimeout" Width="67" Text="500" Height="23" VerticalAlignment="Top" TextChanged="txtbxreadOnceTimeout_TextChanged" PreviewTextInput="txtRFOnTimeout_PreviewTextInput" LostKeyboardFocus="txtbxreadOnceTimeout_LostKeyboardFocus" />
                                                        <CheckBox HorizontalAlignment="Left" VerticalAlignment="Top" ToolTip="Enable fast search" ToolTipService.ShowOnDisabled="True" Content="Fast Search" Height="16" Margin="1,100,0,0" Name="chkEnableFastSearch" Checked="chkEnableFastSearch_Checked" Unchecked="chkEnableFastSearch_Checked" />
                                                    </Grid>
                                                </GroupBox>
                                                <GroupBox Header="Protocols" Name="ProtocolsGroupBox" VerticalAlignment="Top" BorderThickness="1" BorderBrush="Black">
                                                    <StackPanel Name="stackPanel2">
                                                        <StackPanel Name="stackpanel6" Orientation="Horizontal" Margin="2">
                                                            <CheckBox Content="Gen2" Name="gen2CheckBox" Margin="2" IsChecked="True" Checked="gen2CheckBox_CheckedChanged" />
                                                            <CheckBox Content="ISO18000-6B" Name="iso6bCheckBox" Margin="2" Checked="iso6bCheckBox_CheckedChanged" />
                                                            <CheckBox Content="IPX64" Name="ipx64CheckBox" Margin="2" Checked="ipx64CheckBox_CheckedChanged" />
                                                            <CheckBox Content="IPX256" Name="ipx256CheckBox" Margin="2" Checked="ipx256CheckBox_CheckedChanged" />
                                                            <CheckBox Content="ATA" Name="ataCheckBox" Margin="2" Checked="ataCheckBox_CheckedChanged" />
                                                        </StackPanel>
                                                    </StackPanel>
                                                </GroupBox>
                                                <GroupBox Header="Antennas" Name="AntennasGroupBox" BorderThickness="1" BorderBrush="Black">
                                                    <StackPanel Name="stackPanel7">
                                                        <StackPanel Name="stackPanel8" Margin="2" Orientation="Horizontal">
                                                            <CheckBox Content="1" Margin="2" Name="Ant1CheckBox" />
                                                            <CheckBox Content="2" Margin="2" Name="Ant2CheckBox" />
                                                            <CheckBox Content="3" Margin="2" Name="Ant3CheckBox" />
                                                            <CheckBox Content="4" Margin="2" Name="Ant4CheckBox" />
                                                            <CheckBox ToolTipService.ShowOnDisabled="True" ToolTip="Enable antenna detection" Content="Antenna Detection" Margin="14,2,2,2" Name="chkbxAntennaDetection" Checked="chkbxAntennaDetection_Checked" Unchecked="chkbxAntennaDetection_Unchecked" />
                                                        </StackPanel>
                                                        <StackPanel Name="stackPanel20" Orientation="Horizontal" Margin="0,4,0,0">
                                                            <Label Content="Switching Method"  Name="lblAntSwitching" VerticalAlignment="Center"/>
                                                            <StackPanel Name="stackPanel21" Orientation="Vertical" Margin="2">
                                                                <RadioButton VerticalAlignment="Center" ToolTipService.ShowOnDisabled="True" GroupName="rdrAntSwitching" Margin="0,0,2,2" IsChecked="True" Content="Equal Time  "  Name="rdBtnEqualSwitching" >
                                                                    <RadioButton.ToolTip>
                                                                        Assigns equal time to each antenna.
                                                                    </RadioButton.ToolTip>
                                                                </RadioButton>
                                                                <RadioButton VerticalAlignment="Center" ToolTipService.ShowOnDisabled="True" GroupName="rdrAntSwitching"  Content="Dynamic" Name="rdBtnAutoSwitching" >
                                                                    <RadioButton.ToolTip>
                                                                        Dynamically switches through antennas. Goes to next antenna when no more tags found with current.
                                                                    </RadioButton.ToolTip>
                                                                </RadioButton>
                                                            </StackPanel>
                                                        </StackPanel>
                                                    </StackPanel>
                                                </GroupBox>
                                                <GroupBox Header="Embedded ReadData" Name="ReadDataGroupBox" BorderThickness="1" BorderBrush="Black">
                                                    <StackPanel Name="stackPanel17">
                                                        <CheckBox Unchecked="chkEmbeddedReadData_Unchecked" Content="Enable" Margin="2" Name="chkEmbeddedReadData"  ToolTipService.ShowOnDisabled="True" ToolTip="Add an embedded ReadData TagOp to Inventory"/>
                                                        <StackPanel Name="stackPanel18"  Orientation="Horizontal" Visibility="{Binding ElementName=chkEmbeddedReadData, Path=IsChecked, Converter={StaticResource boolToVisible}}">
                                                            <Label Name="lblhiddenembeddedReadvalue" Visibility="Collapsed" Content="" ></Label>
                                                            <Label VerticalAlignment="Center" IsHitTestVisible="False" Content="MemBank" />
                                                            <ComboBox  Margin="2" ToolTip="Select the Memory Bank to read from" SelectedIndex="0"  Name="cbxReadDataBank"  Width="70" >
                                                                <ComboBoxItem Content="TID"/>                                                                
                                                                <ComboBoxItem Content="Reserved"/>
                                                                <ComboBoxItem Content="EPC"/>
                                                                <ComboBoxItem Content="User"/>
                                                            </ComboBox>
                                                            <Label Content="Start"    Name="lblembReadStartAddr"  />
